pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	rectfill(0,0,127,127,1)
	rectfill(0,72,127,72,11)
	rectfill(0,73,127,127,3)
	glitch = 1
	t = 0
	gravity = 5
	friction = 0.9
	jump_time = 7
	jump_strenght_pulse = -10
	jump_strenght_long = -1
	ground = {}
	ground.y = 64
	
	player = {}
	player.x = 72
	player.y = ground.y
	player.horizontal_accel = 0.1
	player.horizontal_speed = 0
	player.horizontal_speed_max = 2
	player.vertical_speed = 0
	player.jump_strenght = 0
	player.jump_time = jump_time
	player.is_jumping = false
	player.on_ground = true
	player.facing = false
	player.run_frames = {1,2,3,3,2,1,4,4}
 player.run_state = 1
 player.is_running = false
 player.sprite = 1
 
	player.update = function()
		parse_run(player)
		parse_jump(player)
	end
end

function parse_run(actor)
	local dir_modifier = 1
	if (btn(0))
	then
		actor.facing = true
		actor.is_running = true
		dir_modifier = -1
	elseif (btn(1))
	then
		actor.facing = false
		actor.is_running = true
	else
		actor.is_running = false
	end
	if (t%2==0)	then	actor.run_state += 1	end
 if (actor.run_frames[actor.run_state] == nil)
 then
  actor.run_state = 1
 end
 if (actor.is_running)
 then
 	actor.horizontal_speed += (actor.horizontal_accel * dir_modifier)
		actor.horizontal_speed = max(min(actor.horizontal_speed, actor.horizontal_speed_max), (actor.horizontal_speed_max * -1))
 	actor.sprite = actor.run_frames[actor.run_state]
	else
 	actor.sprite = 1
 	actor.horizontal_speed *= friction
 	if (abs(actor.horizontal_speed) < 0.1)
 	then
 		actor.horizontal_speed = 0
 	end
 end
 actor.x += (actor.horizontal_speed)
end

function parse_jump(actor)
	if (btn(2) and actor.on_ground and not player.is_jumping)
	then
		sfx(0)
		actor.jump_strenght = jump_strenght_pulse - abs(actor.horizontal_speed) + actor.horizontal_speed_max
		actor.on_ground = false
	elseif (actor.is_jumping and actor.jump_time > 0 and not actor.on_ground)
	then
		actor.jump_strenght += jump_strenght_long
		actor.jump_time -= 1
	end
	if (actor.is_jumping and not btn(2))
	then
		actor.jump_time = 0
	end
	actor.jump_strenght *= 0.9
	actor.vertical_speed = gravity + actor.jump_strenght
	actor.y += actor.vertical_speed
	if (actor.y >= ground.y)
	then
		if (not actor.on_ground)
		then
			sfx(1)
		end
		actor.y = ground.y
		actor.on_ground = true
		actor.jump_time = jump_time
	end
	if (actor.jump_strenght > -2)
	then
		actor.jump_strenght = 0
	end
	actor.is_jumping = btn(2)
	if (not actor.on_ground)
	then
		if (actor.vertical_speed > 0)
		then
			actor.sprite = 5
		else
			actor.sprite = 4
		end
	end
end

function _update()
	t += 1
	player.update()
	if (btnp(5))
	then
		glitch *= -1;
	end
end

function _draw()
	-- cls()
	if (glitch == 1)
	then
		rectfill(0,0,127,127,1)
	end
	rectfill(0,72,127,72,11)
	rectfill(0,73,127,127,3)
	
	print("jump speed " .. player.jump_strenght,1,0,11)
	print("on_ground " .. ((player.y >= ground.y) and "true" or "false"),1,8,11)
	print("is_jumping " .. (player.is_jumping and "true" or "false"),1,16,11)
	print("jump time " .. player.jump_time,1,24,11)	
	print(player.horizontal_speed,1,32,11)	
	print("‹ ‘ to run - ” to jump",1,127-16-1,11)
	print("— to glitch",1,127-8-1,11)
	
	spr(player.sprite,player.x, player.y,1,1,player.facing)
end
